<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>Juulsgaard.Crud.Domain</AssemblyName>
        <RootNamespace>Juulsgaard.Crud.Domain</RootNamespace>
        <PackageId>Juulsgaard.Crud.Domain</PackageId>
        <Title>CRUD Domain Models</Title>
        <Authors>Frederik Sørensen</Authors>
        <RepositoryUrl>https://github.com/Juulsgaard/NugetPackages</RepositoryUrl>
        <RepositoryType>GIT</RepositoryType>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Version>1.0.1</Version>
    </PropertyGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="\" />
    </ItemGroup>

</Project>